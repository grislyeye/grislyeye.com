{
  "name": "grislyeye.com",
  "version": "2.3.3",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@quasibit/eleventy-plugin-sitemap": "^2.1.4",
    "modern-normalize": "^1.1.0",
    "skeleton-css": "^2.0.4",
    "vellum-monster": "^1.0.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "cheerio": "^1.0.0-rc.9",
    "eleventy-plugin-seo": "^0.5.1",
    "htmlhint": "^0.14.2",
    "linkinator": "^2.13.6",
    "rollup": "^2.50.5",
    "rollup-plugin-node-resolve": "^5.2.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0"
  },
  "scripts": {
    "build": "eleventy",
    "deploy": "curl -X POST \"https://api.cloudflare.com/client/v4/zones/6576d55621fb5a1290106d65ff89aee3/purge_cache\" -H \"X-Auth-Email: ricardo@gladwell.me\"  -H \"X-Auth-Key: ${CLOUDFLARE_API_KEY}\" -H \"Content-Type:application/json\" --data '{\"purge_everything\":true}'",
    "clean": "rm -fr _site",
    "start": "eleventy --serve",
    "test": "htmlhint '_site/**/*.html' && stylelint assets/styles/*.css && npx linkinator ./_site -ro --recurse --skip '(.*\\.reddit\\.com)|(.*\\.twitter\\.com)' --verbosity debug"
  }
}
